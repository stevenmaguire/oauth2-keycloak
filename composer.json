{
    "name": "gkasse/oauth2-keycloak",
    "description": "Keycloak OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "gkasse",
            "homepage": "https://github.com/gkasse"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authorisation",
        "keycloak"
    ],
    "require": {
        "league/oauth2-client": "^2.0 <2.3.0",
        "firebase/php-jwt": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Gkasse\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gkasse\\OAuth2\\Client\\Test\\": "test/src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "scripts": {
        "code-sniffer": [
            "phpcs --standard=psr2 src/"
        ],
        "test": [
            "phpunit --coverage-text --coverage-clover=coverage.clover"
        ]
    }
}
